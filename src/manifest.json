
{
  "name": "OU Course Offerings Rate My Professor",
  "description": "shows all professors rate my professor score next to their name in course offerings",
  "version": "1.0",
  "manifest_version": 2,

  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },

  "browser_action": {},
   "content_scripts": [
    {
        "matches": [
            "https://webapps.ohio.edu/classes/*"
        ],
        "js": [
            "jquery.js",
            "content.js"
        ],
        "run_at": "document_end"
    }],

    "permissions": [
      "tabs",
      "storage",
      "https://webapps.ohio.edu/classes/*"
    ]

}
