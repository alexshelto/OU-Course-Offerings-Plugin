{
  "name": "OU Courses Plugin",
  "version": "1.0",
  "description": "Lists instuctor's rate my professor score next to the course",

  "browser_action":{
		"default_popup": "popup.html"
	},

  "content_scripts": [
    {
        "matches": [
            "*://webapps.ohio.edu/classes/results*"
        ],
        "js": [ 
            "jquery.js",
            "content.js"
        ],
        "run_at": "document_end"
    }
],

  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },

  "permissions": ["activeTab"],
  "manifest_version": 2
}